<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC 

"-//OpenSymphony Group//XWork Validator 1.0.2//EN" 

"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	 <field name="yhm">
            <field-validator type="required">
                 <param name="trim">true</param>
                 <message key="yhm.null">dsfagasgda</message>
            </field-validator>
     </field>
     
     <field name="dlpassword">
            <field-validator type="required">
                 <param name="trim">true</param>
                 <message key="dlpassword.null"></message>
            </field-validator>
     </field>
     
     <field name="qrpassword">
            <field-validator type="required">
                  <param name="trim">true</param>
                 <message key="qrpassword.same"></message>
            </field-validator>
     </field>
     
     <field name="email">
        <field-validator type="email">
           <message key="email.email"></message>
     </field-validator>
   </field>
</validators>